CREATE DEFINER=`master`@`localhost` PROCEDURE `conector_alt_pisCofins`(
  in newid int,
  in `newdescricao` VARCHAR(30),
  in `newcofinSt` CHAR(1),
  in `newpiSt` CHAR(1),
  in `newcreditoPis` CHAR(1),
