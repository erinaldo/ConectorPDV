`typeFrete`,
`valorIcms`,
`valorIpi`,
`valorPis`,
`valorCofins`,
`acrecismoValor`,
`descontoValor`,
`valorTotalLiquido`,
`valorTotalNota`,
`valorTotalProdutos`,
`volumes`,
`peso`,
`contribuicaoSocial`,
`quantidadePedido`,
`quantidadeRecebida`,
`impresso`,
`nr_impressao`,
`idTable_Codigo`,
`modNotaFiscal`,
`idSituacaoFiscal`,
`emitiNfe`,
`typenf`,
`msg01`,
`msg02`,
`msg03`,
`valorTotaServico`,
`nr_nota_entrada`,
`serie_entrada`,
`statusNf`,
`restituicao`,
`iss`,
`impostoRenda`,
`funrural`,
`valorTotalFunrural`,
`geraDanfe`,
`condPgto`,
`chave_nfe`,
`protocolo`,
`motivo`,
`versaoNfe`,
`dataHoraRecbNfe`)
VALUES
(
@b,
@c,
@a,
"0",
@d,
default,
4,
default,
default,
default,
default,
default,
default,
default,
default,
default,
default,
default,
default,
default,
default,
default,
default,
