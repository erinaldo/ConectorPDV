  in inc_idtypeReferencia int,
  in inc_empresaContato varchar(40),
  in inc_contato varchar(40),
  in inc_ddd CHAR(4),
  in inc_fone CHAR(8),
  in inc_ramal INT(11),
  in inc_data DATE,
  in inc_observacao VARCHAR(100)
)
begin
insert into clienteReferencia
(
  idcliente,
  idtypeReferencia,
  empresaContato,
  contato,
  ddd,
  fone,
  ramal,
  data,
  observacao
)values
(
  inc_idcliente,
  inc_idtypeReferencia,
  inc_empresaContato,
  inc_contato,
  inc_ddd,
  inc_fone,
  inc_ramal,
  inc_data,
  inc_observacao
);
select last_insert_id();
end $$
/*!50003 SET SESSION SQL_MODE=@TEMP_SQL_MODE */  $$

