  `altValueEntrada` decimal(15,3) NOT NULL DEFAULT '1.000',
  `logicaCredito` char(1) NOT NULL DEFAULT '0' COMMENT '0 | RENDA COMPROVADA\n1 | LIMITE DE CRÉDITO',
