/*!50003 SET @TEMP_SQL_MODE=@@SQL_MODE, SQL_MODE='' */ $$
CREATE DEFINER=`master`@`localhost` PROCEDURE `conector_inc_paramentro_fornecedor_comercial`(
  `inc_idcliente` INT,
  `inc_visita` SMALLINT,
  `inc_analiseCompra` VARCHAR(1),
  `inc_minVolume` INT,
  `inc_valueBay` DECIMAL(15,3),
  `inc_comprador` INT,
  `inc_prazoEntrega` INT,
  `inc_formaPgto` INT(11),
  `inc_banco` INT,
  `inc_agencia` VARCHAR(10),
  `inc_contaCorrente` VARCHAR(15))
begin
  insert into paramentro_fornecedor_comercial(
                                                `idcliente`,
                                                `visita`,
                                                `analiseCompra`,
                                                `minVolume`,
                                                `valueBay`,
                                                `comprador`,
                                                `prazoEntrega`,
                                                `formaPgto`,
                                                `banco`,
                                                `agencia`,
                                                `contaCorrente`)values(
                                                 `inc_idcliente`,
                                                `inc_visita`,
                                                `inc_analiseCompra`,
                                                `inc_minVolume`,
                                                `inc_valueBay`,
                                                `inc_comprador`,
                                                `inc_prazoEntrega`,
                                                `inc_formaPgto`,
                                                `inc_banco`,
                                                `inc_agencia`,
                                                `inc_contaCorrente`
                                                );
                                                select last_insert_id();
  end $$
/*!50003 SET SESSION SQL_MODE=@TEMP_SQL_MODE */  $$

