  `inc_tipo` VARCHAR(1),
  `inc_lancamentoNegativo` VARCHAR(1),
  `inc_inativa` smallint(6))
begin
            INSERT INTO HISTORICO 
                        (DESCRICAO, TIPO, LANCAMENTONEGATIVO, INATIVA) 
                    VALUES 
                        (inc_DESCRICAO, inc_TIPO, inc_LANCAMENTONEGATIVO, inc_INATIVA);
                        select last_insert_id();
end $$
