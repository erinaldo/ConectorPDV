        update 
                setor
        set
                descricao=newdescricao, margem=newmargem,status=newstatus, comissao=newcomissao, premiacao=newpremiacao
        where 
                idsetor=newid;
commit;
END $$
/*!50003 SET SESSION SQL_MODE=@TEMP_SQL_MODE */  $$

