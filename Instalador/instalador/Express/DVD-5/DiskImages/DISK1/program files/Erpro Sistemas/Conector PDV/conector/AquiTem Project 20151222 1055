                                        `dias`=`newdias`,
                                        `porcentagem`=newporcentagem,
                                        `participaLivro`=`newparticipaLivro`
                  where
                                        `idcondicaoPgto`=`newidcondicaoPgto` and `idcondicaoPgtoParcela`=`newidcondicaoPgtoParcela`;

  end $$
