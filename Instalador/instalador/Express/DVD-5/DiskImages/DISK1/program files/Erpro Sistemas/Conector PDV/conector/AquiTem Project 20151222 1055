  `newcet` DECIMAL(15,3) ,
  `newtaxa` DECIMAL(15,3),
  `newparcelamento` int,
  `newUsuarioAlt` int)
begin
            update crediario	set

                                    `idmetodo`=`newidmetodo`,
                                    `idcliente`=`newidcliente`,
                                    `idfuncionario`=`newidfuncionario`,
                                    `valueEntry`=newvalueEntry,
