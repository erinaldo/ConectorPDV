  `cobrancaObrigatoria` smallint(6) NOT NULL DEFAULT '1',
  `taxaObrigatoria` decimal(9,3) NOT NULL DEFAULT '0.000',
  `idFinalizadoraTroco` int(11) NOT NULL DEFAULT '1',
