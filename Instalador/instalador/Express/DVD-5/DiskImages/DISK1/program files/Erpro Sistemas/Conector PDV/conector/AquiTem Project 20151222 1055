  `newidCrediario` double,
  `newidloja` INT,
  `newidmetodo` INT,
  `newidcliente` INT ,
  `newidfuncionario` int,
  `newvalueEntry` DECIMAL(15,3),
  `newtotal` DECIMAL(15,3),
  `newencargos` DECIMAL(15,3),
  `newstatus` CHAR(1),
  `newobservacao` CHAR(100),
  `newentrada` smallint(6),
