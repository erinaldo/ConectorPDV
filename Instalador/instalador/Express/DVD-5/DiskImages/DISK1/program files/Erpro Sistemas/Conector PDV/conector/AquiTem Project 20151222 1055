      mvaMain=newmvaMain,
      `aliquotaInterestadual`=newaliquotaInterestadual,
      `aliquotaInterna`=newaliquotaInterna,
      `mvaAjustada`=newmvaAjustada
      
      where idSpedNcm=ncm;
end $$
