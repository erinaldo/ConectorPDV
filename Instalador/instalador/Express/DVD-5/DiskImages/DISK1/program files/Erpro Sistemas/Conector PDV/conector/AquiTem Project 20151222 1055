begin

        insert into conector.detalhe_reducao(`idLoja`,
  `numeroCaixa`,
  `crz`,
  `mf_letra`,
  `modelo`,
  `operador`,
  `valorAcumulado`, totalizador)
                                                values(`inc_idLoja`,
  `inc_numeroCaixa`,
